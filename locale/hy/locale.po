msgid "plugins.generic.subscriptionSSO.settings.hoursValid.required"
msgstr "Կապի վավեր ժամերի առավելագույն քանակը պետք է լինի ամբողջ թիվ:"

msgid "plugins.generic.subscriptionSSO.settings.hoursValid"
msgstr "Վավեր ժամեր"

msgid "plugins.generic.subscriptionSSO.settings.redirectUrl.required"
msgstr "Վերահղման URL-ը պարտադիր է և պետք է պարունակի վավեր URL:"

msgid "plugins.generic.subscriptionSSO.settings.redirectUrl"
msgstr "Վերահղման URL"

msgid "plugins.generic.subscriptionSSO.settings.resultRegexp.required"
msgstr ""
"Վավերականության կանոնավոր արտահայտությունը պարտադիր դաշտ է և պետք է "
"պարունակի վավեր կանոնավոր արտահայտություն:"

msgid "plugins.generic.subscriptionSSO.settings.resultRegexp"
msgstr "Վավերականության կանոնավոր արտահայտություն"

msgid "plugins.generic.subscriptionSSO.settings.verificationUrl.required"
msgstr "Վավերականության URL-ը պարտադիր է և պետք է պարունակի վավեր URL:"

msgid "plugins.generic.subscriptionSSO.settings.verificationUrl"
msgstr "Վավերականության URL"

msgid "plugins.generic.subscriptionSSO.settings.incomingParameterName.required"
msgstr ""
"Մուտքային պարամետրի անունը պահանջվում է և պետք է պարունակի միայն տառա-թվային "
"նիշեր:"

msgid "plugins.generic.subscriptionSSO.settings.incomingParameterName"
msgstr "Մուտքային պարամետրի անունը"

msgid "plugins.generic.subscriptionSSO.settings.description"
msgstr ""
"Օգտագործեք հետևյալ ձևը՝ արտաքին ծառայությունը կարգավորելու համար, որը "
"կհաստատի բաժանորդագրության կարգավիճակը OJS-ի բաժանորդագրության "
"բովանդակությանը:"

msgid "plugins.generic.subscriptionSSO.subscriptionSSOSettings"
msgstr "Բաժանորդագրության Single Sign-On կարգավորումներ"

msgid "plugins.generic.subscriptionSSO.settings"
msgstr "Կարգավորումներ"

msgid "plugins.generic.subscriptionSSO.description"
msgstr ""
"Այս փլագինը պատվիրակում է բաժանորդագրության ստուգումը արտաքին համակարգին՝ "
"SSO (Single Sign On) նման վարքագծի համար: Երբ հարցումը գալիս է OJS մուտքային "
"պարամետրի անունով, սա պետք է լինի պարամետր, որը OJS-ը չի օգտագործում "
"ներսում, հավելվածը կապ կհաստատի հաստատման URL-ի հետ՝ պարամետրի արժեքով: Եթե "
"հեռակառավարման ծառայության արդյունքը համընկնում է նշված կանոնավոր "
"արտահայտության հետ, օգտվողը կհամարվի վավեր: Եթե ոչ, օգտվողը կվերահղվի նշված "
"URL-ին:"

msgid "plugins.generic.subscriptionSSO.name"
msgstr "SSO բաժանորդագրության փլագին"
